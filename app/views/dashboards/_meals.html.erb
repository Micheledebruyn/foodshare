<h1>Partial for rendering meals current user</h1>
<div class="meals">
  <% @meals.each do |meal| %>
  <div class="meal">
    <%= meal.title %>
    <%= link_to "See Meal", meal_path(meal) %>
    <% if meal.customers.empty? %>
      <%= link_to "Edit Meal", edit_meal_path(meal) %>
    <% end %>
  </div>
  <% end %>
</div>
